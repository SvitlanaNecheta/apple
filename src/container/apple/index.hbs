{{! 
Це код container файлу для index сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/"
 }}

<div class="page" >
  <div class="header">
      {{#with header}}
      <header class="header__contant">
        <div class="header__block">
          <img src="./svg/apple.svg" alt="img_apple" class="header__icon">
          <nav class="header__block">
            {{#each links}}
              <a href="{{url}}" class="header__link">{{text}}</a>
            {{/each}}
          </nav>
        </div>
        <div class="header__block">
          <img src="./svg/search.svg" alt="img_search" class="header__icon">
          <img src="./svg/profil.svg" alt="img_profile" class="header__icon">
        </div>
      </header>
  </div>
  {{#with ad}}
  <div class="header header--light">
    <div class="header__content">
      <div class="header__block" style="padding: 12px 0;">
        <p class="ad__title">{{title}}</p>
        <a href="{{link.url}}" class="product__link" style="font-size: 14px;">{{link.text}}</a>
      </div>
     </div>
  </div>
 {{/with}}
{{/with}}
